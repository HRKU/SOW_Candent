<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
    xmlns:tnt="sap.tnt"
>   
    
            <tnt:ToolHeader
                id="MainHeader"
                class="sapUiTinyMargin"
                style="Standard"
            >
                <Button
                    class="buttonMarginBotton "
                    id="backButton"
                    icon="sap-icon://nav-back"
                    visible="false"
                    press=".onNavBack"
                />
                <Title
                    id="dashboard"
                    text="Dashboard"
                />
                <ToolbarSpacer id="spacer" />

                <Button
                    type="Neutral"
                    press=".onPressDash"
                    id="dashbutton"
                    icon="sap-icon://home"
                    class="sapUiSmallMargin"
                />
                <Button
                    type="Neutral"
                    press=".onPressTable"
                    id="tablebutton"
                    icon="sap-icon://filter-fields"
                    class="sapUiSmallMargin"
                />
                <Avatar
                    id="avatar"
                    displaySize="XS"
                    press=".onAvatarPress"
                    class="sapUiSmallMargin"
                />
                <Button
                    type="Emphasized"
                    press=".onLogout"
                    id="logoutbutton"
                    text="logout"
                    class="sapUiSmallMargin"
                />
            </tnt:ToolHeader>
            

</c:FragmentDefinition>