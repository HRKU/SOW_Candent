<mvc:View
    xmlns:smarttable="sap.ui.comp.smarttable"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="com.candentech.sowtracker.controller.Table"
>
    <Page
        id="apage"
        title="{i18n>title}"
        showHeader="true"
    >
        <!-- SrNo": 1,
"CompanyName": "GlobaleSports.com ",
"Type": "MSA",
"AgreementNo": 4800185,
"ProjectName": "E-eSports Platform",
"AgreementDate": "2024-03-09",
"AgreementStartDate": "2024-03-10",
"AgreementEndDate": "2024-04-08",
"Status": "Active" -->
        <content>
            <Table
                id="projectTable"
                class="sapUiResponsiveMargin"
                width="auto"
                items="{products>/}"
            >
                <headerToolbar>
                    <OverflowToolbar id="toolbar">
                        <ToolbarSpacer id="seprate" />
                        <Button
                            id="Create"
                            text="Create"
                            type="Emphasized"
                            press=".onOpenDialog"
                        />
                    </OverflowToolbar>
                </headerToolbar>
                <columns>
                    <Column id="_IDGenColumn1">
                        <Text
                            id="_IDGenText1"
                            text="Sr No"
                        />
                    </Column>
                    <Column id="_IDGenColumn2">
                        <Text
                            id="_IDGenText2"
                            text="Company Name"
                        />
                    </Column>
                    <Column id="_IDGenColumn3">
                        <Text
                            id="_IDGenText3"
                            text="Project Name"
                        />
                    </Column>
                    <Column id="_IDGenColumn4">
                        <Text
                            id="_IDGenText4"
                            text="Document Type"
                        />
                    </Column>
                    <Column id="_IDGenColumn5">
                        <Text
                            id="_IDGenText5"
                            text="Agreement Date"
                        />
                    </Column>
                    <Column id="_IDGenColumn6">
                        <Text
                            id="_IDGenText6"
                            text="Agreement Start Date"
                        />
                    </Column>
                    <Column id="_IDGenColumn10">
                        <Text
                            id="_IDGenText"
                            text="Agreement End Date"
                        />
                    </Column>
                    <Column id="_IDGenColumn7">
                        <Text
                            id="_IDGenText7"
                            text="Expire In"
                        />
                    </Column>
                    <Column id="_IDGenColumn8">
                        <Text
                            id="_IDGenText8"
                            text="Project Status"
                        />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem1">
                        <cells>
                            <Text
                                id="_IDGenText10"
                                text="{products>SrNo}"
                            />
                            <Text
                                id="_IDGenText11"
                                text="{products>CompanyName}"
                            />
                            <Text
                                id="_IDGenText12"
                                text="{products>ProjectName}"
                            />
                            <Text
                                id="_IDGenText13"
                                text="{products>Type}"
                            />
                            <Text
                                id="_IDGenText14"
                                text="{products>AgreementDate}"
                            />
                            <Text
                                id="_IDGenText15"
                                text="{products>AgreementStartDate}"
                            />
                            <Text
                                id="_IDGenText16"
                                text="{'path': 'products>AgreementEndDate'}"
                            />
                            <Text
                                id="_IDGenText17"
                                text="{products>Status}"
                            />
                            <Text
                                id="_IDGenText18"
                                text="{products>Status}"
                            />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
    </Page>
</mvc:View>
