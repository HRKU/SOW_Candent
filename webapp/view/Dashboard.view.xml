            <List
                id="as"
                items="{path:'docs>/goingToExpire',sorter: {
                path : 'AgreementEndDate',descending:false
                }}"
                headerText="Urgent Expiries"
                headerLevel="H3"
                backgroundDesign="Solid"
                sticky="HeaderToolbar"
            >
                <items>
                    <ObjectListItem
                        type="Navigation"
                        id="list"
                        title="{docs>ProjectName}"
                        number="{path:'docs>AgreementEndDate', formatter:'.formatter.calculateExpiry'}"
                        numberState="{path:'docs>AgreementEndDate', formatter:'.formatter.expiryColor'}"
                        press=".routeToTableViaCard"
                    >
                        <firstStatus>
                            <ObjectStatus
                                id="doctypes"
                                text="{docs>Type}"
                            />
                        </firstStatus>
                        <ObjectAttribute
                            id="_IDGenObjectAttribute1"
                            text="{docs>CompanyName}"
                        />
                    </ObjectListItem>
                </items>
            </List>
        </Panel>

        <VBox
            id="vbox"
            justifyContent="Center"
        >
            <viz:VizFrame
                id="barchartContainer3"
                vizType="pie"
                vizProperties='{
        "title": { "text": "", "alignment" : "center", "style": {"color":"#222222"} },
        "legend": { "visible": true },
        "categoryAxis": { "label": { "linesOfWrap": 2, "angle": 0 } },
        "plotArea": {
            "gridline": { "visible": false },
            "radius": "1.5",
            "dataPointSize": { "max": true },
            "background": { "visible": true, "color": "transparent" },
            "dataLabel":{"type":"color", "visible":true}
        },"general": {
            "background": {
            "visible": true,
            "color": "transparent"
        }
        }
    }'
                uiConfig="{ applicationSet: 'fiori' }"
                selectData=".routeToTableViaChart"
                class="sapUiTinyMargin"
            >
                <viz:dataset>
                    <viz.data:FlattenedDataset
                        id="bardata3"
                        data="{path:'docs>/filtered/len'}"
                    >
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition
                                id="companydimension3"
                                name="type"
                                value="{docs>type}"
                            />
                        </viz.data:dimensions>
                        <viz.data:measures>
                            <viz.data:MeasureDefinition
                                id="barmeasure3"
                                name="Expiry In Days"
                                value="{docs>len}"
                            />
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>

                <viz:feeds>
                    <viz.feeds:FeedItem
                        id='valueAxisFeed3'
                        uid="size"
                        type="Measure"
                        values="Expiry In Days"
                    />
                    <viz.feeds:FeedItem
                        id='categoryAxisFeedC3'
                        uid="color"
                        type="Dimension"
                        values="type"
                    />
                </viz:feeds>
            </viz:VizFrame>
            <FlexBox
                id="_IDGenFlexBox1"
                justifyContent="Center"
            >
                <GenericTile
                    id="_IDGenGenericTile1"
                    class="sapUiSmallMargin sapUiMarginDown "
                    frameType="OneByHalf"
                    header="Total Documents"
                >
                    <TileContent
                        id="_IDGenTileContent1"
                        class=""
                    >
                        <NumericContent
                            id="allExpire"
                            valueColor="Error"
                            value="{docs>/ExpLen}"
                            withMargin="false"
                        />
                    </TileContent>
                </GenericTile>
            </FlexBox>
        </VBox>
    </HBox>
