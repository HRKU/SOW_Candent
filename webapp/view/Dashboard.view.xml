<mvc:View
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data"
    controllerName="com.candentech.sowtracker.controller.Dashboard"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:lf="sap.ui.layout.form"
    xmlns:rowmodes="sap.ui.table.rowmodes"
    xmlns:shapes="sap.gantt.simple.shapes"
    xmlns:config="sap.gantt.config"
    xmlns:axistime="sap.gantt.axistime"
    xmlns:simple="sap.gantt.simple"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:table="sap.ui.table"
    displayBlock="true"
    xmlns:f="sap.f"
    xmlns:cards="sap.f.cards"
>
    <FlexBox
        id="mainFlex"
        width="100%"
        justifyContent="Center"
    >

    
        <VBox
            id="leftSide"
            width="80%"
            justifyContent="Start"
            height="100%"
        >

        <Carousel
    id="carouselSample"
    backgroundDesign="Transparent"
    pages="{trial>/data}"
    showPageIndicator="false">
    <customLayout>
        <CarouselLayout visiblePagesCount="4"/>
    </customLayout>
    
    <ScrollContainer 
        horizontal="false" 
        class="">
        <f:Card>
            <f:header>
                <cards:Header
                    title="Urgent Expiries"
                    iconSrc="sap-icon://alert"
                    iconDisplayShape="Square" />
            </f:header>
                
            <f:content class="sapUiSmallMarginBegin">
                <l:VerticalLayout class="" width="100%">
                    <l:BlockLayout>
                        <l:BlockLayoutRow>
                            <l:BlockLayoutCell  width="1">
                                <VBox class="sapUiTinyMarginBottom" backgroundDesign="Transparent">
                                    <Text text="{trial>client_name}"/>
                                    <GenericTag text="{trial>project_name}" status="Error" design="StatusIconHidden">
                                        <ObjectNumber unit="2 Days"/>
                                    </GenericTag>
                                </VBox>
                            </l:BlockLayoutCell>
                        </l:BlockLayoutRow>
                    </l:BlockLayout>
                </l:VerticalLayout>
            </f:content>
        </f:Card>               
    </ScrollContainer>
</Carousel>

