<mvc:View
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data"
    controllerName="com.candentech.sowtracker.controller.Dashboard"
    xmlns:core="sap.ui.core"
    xmlns:rowmodes="sap.ui.table.rowmodes"
    xmlns:shapes="sap.gantt.simple.shapes"
    xmlns:config="sap.gantt.config"
    xmlns:axistime="sap.gantt.axistime"
    xmlns:simple="sap.gantt.simple"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:table="sap.ui.table"
    displayBlock="true"
    xmlns:f="sap.f"
    xmlns:cards="sap.f.cards"
>
    <viz:VizFrame
        id="barchartContainer"
        vizType="column"
        width="auto"
        vizProperties='{
        "title": { "text": "Close to Expiry" },
        "dataLabel": { "visible": true, "showTotal": true },
        "legend": { "visible": true },
        "categoryAxis": { "label": { "linesOfWrap": 2, "angle": 0 } },
        "plotArea": {
            "gridline": { "visible": false },
            "background": { "visible": false },
            "colorPalette": ["#0057d2","#6C32A9", "#BA066C", "#5D36FF", "#5B738B"],
            "dataPointSize": { "max": true }
        }
    }'
        uiConfig="{ applicationSet: 'fiori' }"
        selectData=".routeToTable"
    >
        <viz:dataset>
            <viz.data:FlattenedDataset
                id="bardata"
                data="{path:'trial>/agreements',sorter : {
                            path : 'Type',descending:true
                            }}"
            >
                <viz.data:dimensions>
                    <viz.data:DimensionDefinition
                        id="companydimension"
                        name="Company Name"
                        value="{docs>CompanyName}"
                    />
                    <viz.data:DimensionDefinition
                        id="projectdimension"
                        name="project name"
                        value="{docs>ProjectName}"
                    />
                    <viz.data:DimensionDefinition
                        id="bardimension1"
                        name="type"
                        value="{docs>Type}"
                    />
                </viz.data:dimensions>
                <viz.data:measures>
                    <viz.data:MeasureDefinition
                        id="barmeasure"
                        name="Expiry In Days"
                        value="{path:'docs>AgreementEndDate', formatter:'.formatter.calculateExpiryChart'}"
                    />
                </viz.data:measures>
            </viz.data:FlattenedDataset>
        </viz:dataset>

        <viz:feeds>
            <viz.feeds:FeedItem
                id='valueAxisFeed'
                uid="valueAxis"
                type="Measure"
                values="Expiry In Days"
            />
            <viz.feeds:FeedItem
                id='categoryAxisFeedC'
                uid="categoryAxis"
                type="Dimension"
                values="Company Name"
            />
            <viz.feeds:FeedItem
                id='categoryAxisFeedP'
                uid="categoryAxis"
                type="Dimension"
                values="project name"
            />
            <viz.feeds:FeedItem
                id='colorFeed'
                uid="color"
                type="Dimension"
                values="type"
            />
        </viz:feeds>
    </viz:VizFrame>

